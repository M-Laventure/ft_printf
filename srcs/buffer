
void	struct_init(t_flags *flag)
{
	t_flags.width = 0;
	t_flags.precision = 0;
	t_flags.diese = 0;
	t_flags.padding = 0;
	t_flags.modif = 0;
	t_flag.minus = 0;
	t_flag.plus = 0;
	t_flag.zero_fill = 0;
	t_flag.id_conv = 0;
}


void	get_args(str_format format, t_flags *flag, va_list va)
{
	id_conv = get_format(str);
	t_flag.param = va_arg(va, char*);
	t_flag.str_bflagss = ft_strsub(format, 0, is_flag_conv(format));
}
